//for even

for (int i=0; i<100;i++){
   if(i%2==0){
   System.out.println(i);
				
 }
}

//for odd 

for (int i=0; i<100;i++){
   if(i%2==1){
   System.out.println(i);
				
 }
}

//Leap year

for (int i=2000; i<2050;i++){
   if(i%4==0){
   System.out.println(i);
 }
}

//plaindrome

String abc = "Vinayak";
String rev = "";
 for (int i=0; i<abc.length() ;i++){
    rev = rev+abc.charAt(i);		
}
    System.out.println(rev);
	
}